<li class="event" data-date="<%= Section.dateRange%>">
    <h3 id="call"><%= Section.title%></h3>
    <div>
        <%- Section.content%>
    </div>
    <% for (const button of Section.buttons) { %>
        <button type="button" class="btn btn-primary">
            <a class="text-white fs-6 link-underline link-underline-opacity-0"
                href="<%= button.url%>"><%= button.text%></a>
        </button>
    <% } %>
</li>